    "@babylonjs/inspector": "^7.52.2",
    "@babylonjs/materials": "^7.52.2",
    "@molcrafts/molrs": "file:../../molrs/molrs-wasm/pkg",
    "tslog": "^4.9.3"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
